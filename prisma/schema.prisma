generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model user_info {
    id         Int    @id @default(autoincrement())
    user_name  String
    password   String
    avatar_url String
    pet_name   String
    sign       String @default("这个人很懒，什么都没写")
    describe   String @default("暂无描述")
}
